! *.color13:      #b294bb

! ! cyan
! *.color6:       #5e8d87
! *.color14:      #8abeb7

! ! white
! *.color7:       #707880
! *.color15:      #c5c8c6

URxvt.iso14755: false
URxvt.iso14755_52: false

xscreensaver.newLoginCommand: dm-tool switch-to-greeter

! xft
Xft.antialias: true
Xft.dpi: 96
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.rgba: rgb
